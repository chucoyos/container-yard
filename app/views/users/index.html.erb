<div>
  <h1 class="font-bold text-4xl">Users#index</h1>
  <p>Find me in app/views/users/index.html.erb</p>

  <% if @users.any? %>
    <ul>
        <% @users.each do |user| %>
          <li>
            <%= user.first_name %>
            <%= user.last_name %>
            <%= user.second_last_name %>
            <%= link_to 'Show', user %>
          </li>
        <% end %>
    </ul>
  <% end %>
</div>
