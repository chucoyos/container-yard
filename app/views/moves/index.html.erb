<div class="flex flex-col px-8 mb-20">

  <h1 class="my-4">Movimientos</h1>

  <div id="moves" class="min-w-full">
    <% @moves.each do |move| %>
      <%= render move %>
      <p class="ml-2">
        <%= link_to "Ver Movimiento", move, class: "btn-primary" %>
      </p>
    <% end %>
  </div>
</div>
